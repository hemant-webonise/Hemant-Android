<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.webonise.comweboniselabhemantandroidsharedpreferences" >

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/image"
        android:label="@string/app_name"
        android:theme="@style/C" >
        <activity
            android:name=".HomeScreen"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".FileOpennerActivity"
            android:label="@string/title_activity_file_openner" >
        </activity>
        <activity
            android:name=".SharedPreferenceActivity"
            android:label="@string/title_activity_shared_preference" >
        </activity>
    </application>

</manifest>
